{
  "manifest_version": 3,
  "name": "Plugin Controller",
  "version": "1.0",
  "description": "Enable or disable other Chrome extensions.",
  "permissions": ["management"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
